{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "tscConfig": {
          "compilerOptions": {
            "resolveJsonModule": true,
            "baseUrl": ".",
            "paths": {
              "~/*": ["src/*"]
            }
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "VALKEY_URL": "@valkey_url"
  }
}
